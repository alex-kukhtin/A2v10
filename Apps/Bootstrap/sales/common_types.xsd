	</xs:simpleType>
	<!--Загальний тип "Код HNDGVSD (№ договору про спільну (сумісну) діяльність)"-->
	<xs:simpleType name="DGHNDGVSD">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINUM (податковий номер платника, що проводить управління майном)"-->
	<xs:simpleType name="DGHTINUM">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код HDDGVUM (Дата договору про управління майном )"-->
	<xs:simpleType name="DGHDDGVUM">
		<xs:restriction base="DGDate"/>
	</xs:simpleType>
	<!--Загальний тип "Код HNDGVUM (№ договору про управління майном)"-->
	<xs:simpleType name="DGHNDGVUM">
