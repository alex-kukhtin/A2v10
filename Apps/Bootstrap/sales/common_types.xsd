			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Рік випуску, створення, ..."-->
	<xs:simpleType name="DGMadeYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1800"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Назва підприємства"-->
	<xs:simpleType name="DGHNAME">
		<xs:restriction base="xs:string">
