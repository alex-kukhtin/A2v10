		<xs:annotation>
			<xs:documentation>
				розділ Декларації (А,Б,В,Г)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[АБВГ]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--тип "акциз код операція в декларації"-->
	<xs:simpleType name="DGJ02095Ind2">
		<xs:annotation>
			<xs:documentation>
				операція в Декларації (А1-А99,Б1-Б99,В...,Г...)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([АБВГ]([1-9]{1}[0-9]{0,1}){1}){1}"/>
		</xs:restriction>
	</xs:simpleType>
