	<xs:simpleType name="DGdecimalZero">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="0(\.0{1,2}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "проценти розподілу, питома вага, числові дані, можливо з 2 знаками після коми, від 0 до 100 включно"-->
	<xs:simpleType name="DGpercentAlloc">
		<xs:restriction base="DGdecimal2">
			<xs:minInclusive value="0.00"/>
			<xs:maxInclusive value="100.00"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 0 знаки після коми"-->
	<xs:simpleType name="DGdecimal0">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+(\.0{1,2}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 1 знаки після коми"-->
	<xs:simpleType name="DGdecimal1">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{1,2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 2 знаки після коми"-->
	<xs:simpleType name="DGdecimal2">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Позитивні числові дані 2 знаки після коми"-->
	<xs:simpleType name="DGdecimal2_P">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[0-9]+\.[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 3 знаки після коми"-->
	<xs:simpleType name="DGdecimal3">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 4 знаки після коми"-->
	<xs:simpleType name="DGdecimal4">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 5 знаки після коми"-->
	<xs:simpleType name="DGdecimal5">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 6 знаки після коми"-->
	<xs:simpleType name="DGdecimal6">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 6 знаки після коми спец"-->
	<xs:simpleType name="DGdecimal6_R">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+(\.[0-9]{1,6}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 12 знаки після коми спец"-->
	<xs:simpleType name="DGdecimal12_R">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+(\.[0-9]{1,12}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 13 знаки після коми спец"-->
	<xs:simpleType name="DGdecimal13_R">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+(\.[0-9]{1,13}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "5-10-15 неоподатковуваних мінімумів "-->
	<xs:simpleType name="DG5-10-15nmg">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="10"/>
			<xs:enumeration value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ДРФО фізичної особи допускається числовий код, або № паспорта"-->
	<xs:simpleType name="DGDrfo">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ДРФО фізичної особи допускається числовий код, або № паспорта з БК для Пенсійного фонду"-->
	<xs:simpleType name="DGDrfo_pfu">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|БК[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Номер облікової картки застрахованої особи (номер ЗО)(Пенсійний Фонд України)"-->
	<xs:simpleType name="DGNUMZO">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{5,10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯабвгдеєжзиіклмнопрстуфхцчшщюя]{3} [0-9]{6}|БК[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6}|F[A-Za-zАБВГҐДЕЄЖЗИІЇКЛМНОПРСТУФХЦЧШЩЮЯЬЫЪЁЭабвгґдеєжзиіїклмнопрстуфхцчшщюяьыъёэ0-9]+|П[0-9]{9})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "№ договору про добровільну  участь  у системі загальнообов'язкового державного соціального страхування (Пенсійний Фонд України)"-->
	<xs:simpleType name="DGNUMdduszss">
		<xs:restriction base="xs:string">
			<xs:pattern value="([АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯабвгдеєжзиіклмнопрстуфхцчшщюя]{3} [0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Ознака сезон (Пенсійний Фонд України)"-->
	<xs:simpleType name="DGSEAZON">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "серія та № паспорта" (HPASS)-->
	<xs:simpleType name="DGPass">
		<xs:restriction base="xs:string">
			<xs:maxLength value="9"/>
			<xs:pattern value="[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6}|[0-9]{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "серія паспорта" (HPASSSER)-->
	<xs:simpleType name="DGPassSer">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:pattern value="[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "серія паспорта" в колонці-->
	<xs:complexType name="PassSerColumn">
		<xs:simpleContent>
			<xs:extension base="DGPassSer">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "№ паспорта" (HPASSNUM)-->
	<xs:simpleType name="DGPassNum">
		<xs:restriction base="xs:string">
			<xs:maxLength value="9"/>
			<xs:pattern value="[0-9]{6}|[0-9]{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "№ паспорта" в колонці-->
	<xs:complexType name="PassNumColumn">
		<xs:simpleContent>
			<xs:extension base="DGPassNum">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Код ЄДРПОУ (тiльки числа-породжений від string, податковий номер)"-->
	<xs:simpleType name="DGNLong">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ЄДРПОУ (ДРФО підприємця: числа, або № паспорта)"-->
	<xs:simpleType name="DGLong">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Реєстраційний номер облікової картки платника податків (10 цифр з лідируючим нулем) або серія та номер паспорта або номер паспорта у вигляді ID картки"-->
	<xs:simpleType name="DGDRFO_10">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6}|[0-9]{9})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ЄДРПОУ (ДРФО підприємця: числа, або № паспорта з БК) Для Пенсійного фонду"-->
	<xs:simpleType name="DGLong_pfu">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|БК[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип " Код HTINJ (Ідентифікаційний код платника згідно з ЄДРПОУ)"-->
	<xs:simpleType name="DGHTINJ">
		<xs:restriction base="DGNLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINF (Ідентифікаційний номер платника згідно з ДРФО)"-->
	<xs:simpleType name="DGHTINF">
		<xs:restriction base="DGLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINF (Ідентифікаційний номер платника згідно з ДРФО, або №паспорта з БК) Для Пенсійного фонду"-->
	<xs:simpleType name="DGHTINF_pfu">
		<xs:restriction base="DGLong_pfu"/>
	</xs:simpleType>
