		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Номер факса"-->
	<xs:simpleType name="DGHFAX">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9 ()\-+\.,;]{4,}"/>
		</xs:restriction>
	</xs:simpleType>
